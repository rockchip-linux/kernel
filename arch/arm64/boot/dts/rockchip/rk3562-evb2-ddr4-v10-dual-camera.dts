&pinctrl {
	cam {
		mipicsi0_pwr: mipicsi0-pwr {
			rockchip,pins =
				/* camera power en */
				<4 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

