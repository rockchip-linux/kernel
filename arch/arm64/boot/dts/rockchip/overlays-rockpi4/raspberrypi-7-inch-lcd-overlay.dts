/dts-v1/;
/plugin/;

/ {
	model = "ROCK PI 4B";
	compatible = "rockchip,rockpi","rockchip,rk3399";

	fragment@0 {
		target = <&dsi>;

		__overlay__ {
			status = "okay";
		};
	};

	fragment@1 {
		target = <&dsi1>;

		__overlay__ {
			rockchip,dual-channel = <&dsi>;
			status = "okay";
			
			panel: panel@0 {
			compatible ="rockpi,tc358762";
			reg = <0>;
			status = "okay";
	
			};
		};
	};

	fragment@2 {
		target = <&i2c1>;

		__overlay__ {
			rockpi_mcu: rockpi_mcu@45 {
				status = "okay";
			};

			rockpi_ft5406: rockpi_ft5406@38 {
				status = "okay";
			};
		};
	};
};
