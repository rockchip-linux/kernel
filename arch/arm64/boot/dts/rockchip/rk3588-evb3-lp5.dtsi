		29 00 02 22 2F
		/* 90Hz */
		29 00 02 23 11
		/* FR0 (60Hz) */
		29 00 02 24 00
		29 00 02 25 75
		29 00 02 27 00
		29 00 02 28 1A
		29 00 02 29 00
		29 00 02 2A 1A
		29 00 02 2B 00
		29 00 02 2D 1A
		/* FR1 (90Hz) */
		29 00 02 2F 00
		29 00 02 30 55
		29 00 02 32 00
		29 00 02 33 1A
		29 00 02 34 00
		29 00 02 35 1A
		29 00 02 36 00
		29 00 02 37 1A
		/* CMD2,Page3 */
		29 00 02 FF 23
		29 00 02 FB 01
		/* DBV=12 bit */
		29 00 02 00 80
		/* PWM frequency */
		29 00 02 07 00
		/* CMD3,PageA */
		29 00 02 FF E0
		29 00 02 FB 01
		/* VCOM Driving Ability */
		29 00 02 14 60
		29 00 02 16 C0
		/* CMD3,PageB */
		29 00 02 FF F0
		29 00 02 FB 01
		/* slave osc workaround */
		29 00 02 3A 08
		/* CMD3,PageC */
		29 00 02 FF D0
		29 00 02 FB 01
		29 00 02 1C 88
		29 00 02 1D 08
		/* CMD1 */
		29 00 02 FF 10
		29 00 02 FB 01
		/* Only Write Slave */
		29 00 02 B9 01
		/* CMD2,Page0 */
		29 00 02 FF 20
		29 00 02 FB 01
		29 00 02 18 40
		/* CMD1 */
		29 00 02 FF 10
		29 00 02 FB 01
		/* Write Master & Slave */
		29 00 02 B9 02
		29 00 02 35 00
		29 00 03 51 00 FF
		29 00 02 53 24
		29 00 02 55 00
		29 00 02 BB 13
		/* VBP+VFP=121 */
		29 00 06 3B 03 5F 1A 04 04
		/* CMD2,Page5 */
		29 00 02 FF 25
		/* FRM */
		29 00 02 EC 00
		/* CMD1 */
		29 00 02 FF 10
		29 00 02 FB 01
		05 FF 01 11
		05 FF 01 29
	];

	panel-exit-sequence = [
		05 00 01 28
		05 00 01 10
	];

