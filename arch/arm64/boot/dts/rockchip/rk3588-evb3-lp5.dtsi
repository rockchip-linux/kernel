			hback-porch = <32>;
			vfront-porch = <26>;
			vsync-len = <2>;
			vback-porch = <93>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <0>;
		};
	};
};

