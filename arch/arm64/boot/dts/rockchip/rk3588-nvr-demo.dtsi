&i2c3 {
	status = "okay";
	es8311: es8311@18 {
		status = "okay";
		compatible = "everest,es8311";
		reg = <0x18>;
		#sound-dai-cells = <0>;
		adc-pga-gain = <6>;     /* 18dB */
		adc-volume = <0xbf>;    /* 0dB */
		dac-volume = <0xbf>;    /* 0dB */
		aec-mode = "adc left, adc right";
		clocks = <&mclkout_i2s0>;
		clock-names = "mclk";
		assigned-clocks = <&mclkout_i2s0>;
		assigned-clock-rates = <12288000>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2s0_mclk>;
	};
};

